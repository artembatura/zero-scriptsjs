{
  "name": "example-react-astroturf",
  "private": true,
  "scripts": {
    "build": "run build",
    "start": "run start",
    "watch": "run watch"
  },
  "dependencies": {
    "astroturf": "1.0.0-beta.16",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@zero-scripts/core": "workspace:*",
    "@zero-scripts/plugin-webpack-babel": "workspace:*",
    "@zero-scripts/plugin-webpack-css": "workspace:*",
    "@zero-scripts/plugin-webpack-react": "workspace:*",
    "@zero-scripts/plugin-webpack-spa": "workspace:*",
    "babel-plugin-named-asset-import": "^0.3.7",
    "webpack": "^5.10.0"
  },
  "zero-scripts": {
    "plugin-webpack-babel": {
      "jsLoaders": [
        {
          "loader": "astroturf/loader",
          "options": {
            "extension": ".astro.css"
          }
        }
      ]
    },
    "plugin-webpack-css": {
      "styleLoaders": [
        {
          "test": "\\.astro\\.css",
          "loader": "astroturf/css-loader"
        }
      ]
    }
  }
}
